@CHARSET "UTF-8";
*{font-size:12px; font-family:Tahoma,Verdana,微软雅黑,新宋体}
#position_info_table{
	font-size:15px;
	font-family:Tahoma,Verdana,微软雅黑,新宋体;
	position:absolute;
	top:30px;
	left:50px;
	width:500px;
}
