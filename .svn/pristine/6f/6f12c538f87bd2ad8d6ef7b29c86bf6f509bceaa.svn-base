@CHARSET "UTF-8";
*{font-size:15px; font-family:Tahoma,Verdana,微软雅黑,新宋体}
#position_info_table{
	font-size:15px;
	font-family:Tahoma,Verdana,微软雅黑,新宋体;
	position:absolute;
	top:90px;
	left:50px;
	width:570px;
}
#alert_table{
	font-size:14px;
	font-family:Tahoma,Verdana,微软雅黑,新宋体;
	position:absolute;
	top:90px;
	left:650px;
	width:400px;
}
.alert_table_tr{
	height:36px;	
}
.alert_img{
	width:36px;
	height:36px;
}
